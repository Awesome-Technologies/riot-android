<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_height="match_parent"
    android:layout_width="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <im.vector.view.CaseCreateDataPatientView
            android:id="@+id/case_create_data_patient_view"
            android:layout_height="wrap_content"
            android:layout_width="match_parent">

            <include layout="@layout/case_data_list_divider"
                android:layout_alignParentBottom="true"
                android:layout_height="1dp"
                android:layout_width="match_parent" />

        </im.vector.view.CaseCreateDataPatientView>

        <im.vector.view.CaseCreateDataLabelValueView
            android:id="@+id/case_create_data_recipient_view"
            android:layout_height="wrap_content"
            android:layout_width="match_parent">

            <include layout="@layout/case_data_list_divider"
                android:layout_alignParentBottom="true"
                android:layout_height="1dp"
                android:layout_width="match_parent" />

        </im.vector.view.CaseCreateDataLabelValueView>

        <im.vector.view.CaseCreateDataLabelValueView
            android:id="@+id/case_create_data_severity_view"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"/>

        <im.vector.view.CaseDataSectionHeaderView
            android:id="@+id/case_create_message_section_header"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" />

        <im.vector.view.CaseCreateDataLabelValueView
            android:id="@+id/case_create_data_title_view"
            android:layout_height="wrap_content"
            android:layout_width="match_parent">

            <include layout="@layout/case_data_list_divider"
                android:layout_alignParentBottom="true"
                android:layout_height="1dp"
                android:layout_width="match_parent" />

        </im.vector.view.CaseCreateDataLabelValueView>

        <im.vector.view.CaseCreateDataNoteView
            android:id="@+id/case_create_data_note_view"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"/>

        <im.vector.view.CaseDataSectionHeaderView
            android:id="@+id/case_create_data_section_header"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" />

        <im.vector.view.CaseCreateDataLabelValueView
            android:id="@+id/case_create_data_anamnesis_view"
            android:layout_height="wrap_content"
            android:layout_width="match_parent">

            <include layout="@layout/case_data_list_divider"
                android:layout_alignParentBottom="true"
                android:layout_height="1dp"
                android:layout_width="match_parent" />

        </im.vector.view.CaseCreateDataLabelValueView>

        <im.vector.view.CaseCreateDataLabelValueView
            android:id="@+id/case_create_data_vitals_view"
            android:layout_height="wrap_content"
            android:layout_width="match_parent">

            <include layout="@layout/case_data_list_divider"
                android:layout_alignParentBottom="true"
                android:layout_height="1dp"
                android:layout_width="match_parent" />

        </im.vector.view.CaseCreateDataLabelValueView>

        <im.vector.view.CaseCreateDataPicturesView
            android:id="@+id/case_create_data_pictures_view"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" />

    </LinearLayout>
</ScrollView>